    "MediumRange": 115,
    "HighRange": 122,
    "QuietProfile": true
  },
  "schema": {
    "CorF": "str",
    "LowRange": "int(0,255)",
    "MediumRange": "int(0,255)",
    "HighRange": "int(0,255)",
    "QuietProfile": "bool"
  }
