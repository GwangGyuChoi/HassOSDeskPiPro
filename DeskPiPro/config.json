{
  "name": "DeskPi Pro Active Cooling",
  "version": "0.4",
  "slug": "deskpipro",
  "description": "This adjusts the temp of DeskPi Pro by fan control",
  "arch": ["armhf", "armv7", "aarch64"],
  "startup": "application",
  "full_access": "no",
  "stage": "stable",
  "ingress": true,
  "auth_api": true,
  "homeassistant_api": true,
  "boot": "auto",
  "devices": ["/dev/ttyUSB0:/dev/ttyUSB0:rw","/dev/ttyUSB1:/dev/ttyUSB1:rw","/dev/ttyUSB2:/dev/ttyUSB2:rw","/dev/ttyUSB3:/dev/ttyUSB3:rw","/dev/ttyUSB4:/dev/ttyUSB4:rw"],
  "host_network": false,
  "map": ["config", "ssl", "addons", "backup", "share", "media"],
  "options": {
    "CorF": "F",
    "LowRange": 90,
    "MediumRange": 115,
    "HighRange": 122,
    "QuietProfile": true,
    "SerialDevice":"/dev/ttyUSB0"
  },
  "schema": {
    "CorF": "str",
    "LowRange": "int(0,255)",
    "MediumRange": "int(0,255)",
    "HighRange": "int(0,255)",
    "QuietProfile": "bool",
    "SerialDevice":"str"
  }
}
